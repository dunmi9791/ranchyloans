<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="1">
    <record id="increment_loan_sequence" model="ir.sequence">
    <field name="name">Loan Incrementation</field>
    <field name="code">increment_loan</field>
    <field name="prefix">Loan/%(range_year)s/</field>
    <field name="padding">5</field>
    <field name="company_id" eval="False"/>
</record>
    <record id="increment_member_sequence" model="ir.sequence">
    <field name="name">Member Incrementation</field>
    <field name="code">increment_member</field>
    <field name="prefix">Member/%(range_year)s/</field>
    <field name="padding">5</field>
    <field name="company_id" eval="False"/>
</record>
    <record id="increment_request_sequence" model="ir.sequence">
    <field name="name">Request Incrementation</field>
    <field name="code">increment_request</field>
    <field name="prefix">request </field>
    <field name="padding">5</field>
    <field name="company_id" eval="False"/>
</record>
</data>

</odoo>
